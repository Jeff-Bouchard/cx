package main

func dot(x []i32, y []i32) (r i32) {
	if len(x) != len(y) {
		r = 0
		return
	}

	var tmp i32
	l := len(x)
	for i := 0; i < l; i++ {
		tmp = i32.mul(x[i], y[i])
		r += tmp
	}
}

